@model Employee
<h1>Employee Detail</h1>
<ul class="list-group">
  <ul class="list-group">
  <li class="list-group-item">First Name: @Model.FirstName</li>
  <li class="list-group-item">Last Name: @Model.Surname</li>
  <li class="list-group-item">Address: @Model.Address</li>
  <li class="list-group-item">Gender: @Model.Gender</li>
  <li class="list-group-item">Salary: Rs. @Model.Salary</li>
</ul> 